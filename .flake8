[flake8]
ignore = B007, B008, D203, W503, C812, C815, CFQ002
exclude = .git, __pycache__, migrations, venv
per-file-ignores = __init__.py:F401,tests/*__init__.py:F401,F403,tests/*:E501
max-line-length = 120
max-function-length = 80
max-complexity = 10
inline-quotes = "
max-cognitive-complexity = 10
